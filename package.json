{
  "name": "torn-abroad-stock-predictor",
  "version": "2.0.0",
  "description": "Firefox extension to predict Torn City foreign stock quantities",
  "main": "tornstocklogger.js",
  "scripts": {
    "build": "npm run generate-icons && web-ext build",
    "start": "web-ext run",
    "lint": "eslint . && web-ext lint",
    "test": "jest",
    "format": "prettier --write .",
    "generate-icons": "node scripts/generate-icons.js",
    "dev": "web-ext run --firefox=firefox-developer-edition",
    "watch": "web-ext run --firefox=firefox-developer-edition --watch-file=**/*.{js,json,html,css}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/torn-abroad-stock-predictor.git"
  },
  "keywords": [
    "torn",
    "torn-city",
    "firefox-extension",
    "stock-predictor",
    "yata"
  ],
  "author": "MrChurchh [3654415] <0xxzerocoolxx0@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/yourusername/torn-abroad-stock-predictor/issues"
  },
  "homepage": "https://github.com/yourusername/torn-abroad-stock-predictor#readme",
  "devDependencies": {
    "web-ext": "^7.11.0",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "sharp": "^0.33.2",
    "@types/jest": "^29.5.11",
    "jest-environment-jsdom": "^29.7.0",
    "eslint-config-prettier": "^9.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
} 